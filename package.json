{
  "name": "EHR-UI-app",
  "version": "1.0.0",
  "description": "therapy management UI app",
  "scripts": {
    "start": "npm run start-prod-server",
    "prestart": "npm run build",
    "start-prod-server": "better-npm-run start-prod-server",
    "start-dev-server": "better-npm-run start-dev-server",
    "build-client": "better-npm-run build-client",
    "build": "better-npm-run build",
    "dev": "concurrently --colors --kill-others \"npm run start-dev-server\" \"npm run client:watch\"",
    "test": "karma start",
    "test-node": "mocha --compilers js:babel-core/register --require ./src/__tests__/test_helper.js $(find src -name '*-test.js')",
    "lint": "esw --fix -f node_modules/eslint-friendly-formatter src -w -c ./.eslintrc",
    "client:watch": "concurrently --kill-others \"npm run build-client\" \"npm run lint\"",
    "test-node:watch": "npm run test -- --watch"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "production"
      }
    },
    "start-prod-server": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "production"
      }
    },
    "start-dev-server": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "development",
        "PORT": 3001
      }
    },
    "build-client": {
      "command": "node ./webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joofsh/ehr-ui"
  },
  "keywords": [
    "react",
    "dc-resources"
  ],
  "license": "MIT",
  "homepage": "https://github.com/joofsh/ehr-ui",
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta10",
    "babel-loader": "^6.0.1",

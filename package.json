{
  "name": "EHR-UI-app",
  "version": "1.0.0",
  "description": "therapy management UI app",
  "scripts": {
    "start-prod-server": "better-npm-run start-prod-server",
    "start-dev-server": "better-npm-run start-dev-server",
    "build-client": "better-npm-run build-client",
    "build": "better-npm-run build",
    "dev": "concurrent --colors --kill-others \"npm run client:watch\" \"npm run start-dev-server \"",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "lint": "esw -c .eslintrc -f node_modules/eslint-friendly-formatter src -w -c ./.eslintrc",
    "client:watch": "concurrent --kill-others \"npm run build-client\" \"npm run lint\"",
    "test:watch": "npm run test -- --watch"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "production"
      }
    },
    "start-prod-server": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev-server": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "development",
        "PORT": 3001
      }
    },
    "build-client": {
      "command": "node ./webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joofsh/ehr-ui"
  },
  "keywords": [
    "react",
    "dc-resources"
  ],
  "license": "MIT",
  "homepage": "https://github.com/joofsh/ehr-ui",
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta10",
    "babel-loader": "^6.0.1",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "concurrent": "^0.3.2",
    "css-loader": "^0.23.1",
    "eslint": "v1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.6.2",
    "eslint-stylish-config": "0.0.2",
    "eslint-watch": "^2.1.7",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "pretty-error": "^2.0.0",
    "react-hot-loader": "^1.3.0",
    "strip-loader": "^0.1.2",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {

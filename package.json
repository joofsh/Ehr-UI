    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "better-npm-run": "0.0.5",
    "body-parser": "^1.14.2",
    "bootstrap": "^3.3.6",
    "bootstrap-loader": "^1.0.1",
    "bootstrap-sass": "^3.3.6",
    "classnames": "^2.2.3",
    "clean-webpack-plugin": "^0.1.8",
    "compression": "^1.6.0",
